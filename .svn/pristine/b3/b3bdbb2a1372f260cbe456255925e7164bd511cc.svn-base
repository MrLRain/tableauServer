<!-- Definition of request and response elements that wrap all payloads. Definition of the error message payload. -->
<xs:element name="tsRequest">
<xs:complexType>
<xs:choice>
<xs:element name="connection" type="connectionType"/>
<xs:element name="connections" type="connectionListType"/>
