#mongo服务的端口号
spring.data.mongodb.port=27017
#mongo服务的地址
spring.data.mongodb.host=47.104.210.63
#mongo服务的登录名称
spring.data.mongodb.username=vk-data
#mongo服务的密码
spring.data.mongodb.password=123456
#此人的验证数据库
spring.data.mongodb.authentication-database=vk-data
#待连接数据库
spring.data.mongodb.database=vk-data
#mongo最大的池子数
spring.data.mongodb.option.max-connection-per-host=100
#最大支持五个线程并发mongo连接
spring.data.mongodb.option.threads-allowed-to-block-for-connection-multiplier=5
#套接字选择超时时间
spring.data.mongodb.option.server-selection-timeout=30000
#池化连接的最大空闲时间。零值表示空闲时间没有限制。超出其空闲时间的池化连接将被关闭，并在必要时由新连接替换。
spring.data.mongodb.option.max-connection-idle-time=500000
#最大等待时间
spring.data.mongodb.option.max-wait-time=120000
#连接超时
spring.data.mongodb.option.connect-timeout=10000
# 设置套接字超时
spring.data.mongodb.option.socket-timeout=50000
#会话一直存在的会引发socket异常
spring.data.mongodb.option.socket-keep-alive=false
#设置启用SSL
spring.data.mongodb.option.ssl-enabled=false
#定义是否允许使用无效的主机名。
spring.data.mongodb.option.ssl-invalid-host-name-allowed=false
#设置驱动程序注册的JMX bean是否应始终为MBean，无论VM是Java还是更高版本。
spring.data.mongodb.option.always-use-m-beans=false
#可配置默认值为15毫秒
spring.data.mongodb.option.local-threshold=15
spring.data.mongodb.option.max-connection-life-time=500000


